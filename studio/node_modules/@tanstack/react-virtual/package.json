    "build/umd/*",
    "src"
  ],
  "devDependencies": {
    "@testing-library/react": "^12.1.2",
    "@types/react": "17.0.45",
    "@types/react-dom": "17.0.17",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "resize-observer-polyfill": "^1.5.1"
  },
  "dependencies": {
    "@tanstack/virtual-core": "3.0.0-beta.29"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
